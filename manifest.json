{
    "name": "GMV AI Safety App",
    "description": "AI Safety App",
    "version": "1.0",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "permissions": [
      "*://*/*",
      "contextMenus",
      "notifications",
      "activeTab",
      "downloads",
      "declarativeContent",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage",
      "tabs"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
          }
      },
    "icons": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    },
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/@tensorflow/tfjs@latest https://cdn.jsdelivr.net/npm/@tensorflow-models/coco-ssd 'unsafe-eval'; object-src 'self'"
  }